    local success, err = pcall(function()
        DoodleInfo = require(DoodleInfoModule)
    end)
    
    if success then
        print("‚úì DoodleInfo loaded")
    else
        warn("‚ùå Failed to load DoodleInfo:", err)
        return
    end
else
    warn("‚ùå DoodleInfo module not found!")
    return
end

-- Build Doodle list
