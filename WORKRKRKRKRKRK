filterCorner.CornerRadius = UDim.new(0, 8)
filterCorner.Parent = activeFiltersLabel

local function UpdateActiveFilters()
    local filters = {}
    if Settings.TargetSpecies ~= "" then table.insert(filters, "Species: " .. Settings.TargetSpecies) end
    if Settings.MinStars >= 1 then table.insert(filters, "Exact Stars: " .. Settings.MinStars .. "‚≠ê") end
    if Settings.OnlyMisprint then table.insert(filters, "Misprint Only") end
    if Settings.OnlyHiddenTrait then table.insert(filters, "Hidden Trait Only") end
    if Settings.TargetAbility ~= "" then table.insert(filters, "Ability: " .. Settings.TargetAbility) end
    if Settings.TargetGender ~= "Any" then table.insert(filters, "Gender: " .. Settings.TargetGender) end
    if Settings.TargetSkin ~= "Any" then table.insert(filters, "Skin: " .. Settings.TargetSkin) end
    
    if #filters == 0 then
        activeFiltersLabel.Text = "üéØ ACTIVE FILTERS:\nNone - Set filters or script won't stop!"
        activeFiltersLabel.BackgroundColor3 = Color3.fromRGB(150, 50, 50)
    else
        activeFiltersLabel.Text = "üéØ ACTIVE FILTERS (ALL must match):\n" .. table.concat(filters, ", ")
        activeFiltersLabel.BackgroundColor3 = Color3.fromRGB(60, 100, 60)
    end
end

AddDropdown(filtersTab, "üîç Search Doodle Name...", DoodleNames, function(text)
    Settings.TargetSpecies = text
    print("Target:", text == "" and "Any" or text)
    UpdateActiveFilters()
end)

AddSlider(filtersTab, "Exact Stars ‚≠ê (Any/1-6)", -1, 6, -1, function(value)
    Settings.MinStars = value
    if value == -1 then
        print("Stars: Any (don't care)")
    else
        print("Exact Stars:", value, "‚≠ê ONLY")
    end
    UpdateActiveFilters()
end)

