activeFiltersLabel.TextSize = 11
activeFiltersLabel.Font = Enum.Font.Gotham
activeFiltersLabel.TextWrapped = true
activeFiltersLabel.TextYAlignment = Enum.TextYAlignment.Top
activeFiltersLabel.Parent = filtersTab

local filterPadding = Instance.new("UIPadding")
filterPadding.PaddingLeft = UDim.new(0, 10)
filterPadding.PaddingRight = UDim.new(0, 10)
filterPadding.PaddingTop = UDim.new(0, 8)
filterPadding.PaddingBottom = UDim.new(0, 8)
filterPadding.Parent = activeFiltersLabel

